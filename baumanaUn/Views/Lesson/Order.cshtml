@model IEnumerable<baumanaUn.Models.Order>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p></p>
<table id="order" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th class="text-center">Id</th>
            <th class="text-center">ClientName</th>
            <th class="text-center">Phone</th>
            <th class="text-center">Email</th>
            <th class="text-center">OrderCreated</th>
            <th class="text-center">GradeId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var names in Model)
        {
            <tr>

                <td class="vert-align text-center">@names.Id</td>
                <td class="vert-align text-center">@names.ClientName</td>
                <td class="vert-align text-center">@names.Phone</td>
                <td class="vert-align text-center">@names.Email</td>
                <td class="vert-align text-center">@names.OrderCreated</td>
                <td class="vert-align text-center">@names.GradeId</td>
            </tr>
        }
    </tbody>
</table>